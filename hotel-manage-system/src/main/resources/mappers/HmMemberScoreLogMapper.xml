  <select id ="queryHmMemberScoreLog" resultType="java.util.HashMap" parameterType="map">
    SELECT
	m.memberName,
	m.memberBirthday,
	g.phone,
	g.memberScore,
	CASE g.`status`
WHEN 1 THEN
	'增加'
WHEN 2 THEN
	'消耗'
ELSE
	''
END status
FROM
	hm_member_score_log g
INNER JOIN hm_member m ON g.phone = m.memberPhone
where 1=1
<if test="map.name != null">
  and m.memberName like concat(concat('%',#{map.name}),'%')
</if>
<if test="map.phone != null">
